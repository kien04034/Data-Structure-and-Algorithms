#include<bits/stdc++.h>
using namespace std;

int c[100],d1[100],d2[100];
int n,res;
int X[100];
void in(){
	int a[100][100];
	memset(a,0,sizeof(a));
	for(int i = 1; i <= n ; i++){
		a[i][X[i]] = 1;
	}
	for(int i = 1 ; i <= n ; i++){
		for(int j = 1; j <= n; j++){
			cout <<a[i][j];
		}
		cout << endl;
	}
}

void Try(int i){
	for(int j = 1; j <= n; j++){
		if(!c[j] && !d1[i - j + n] && !d2[i + j - 1]){
			X[i] = j;
			c[j] = d1[i - j + n] = d2[i + j -1] = 1;
			if(i == n){
				++res;
				in();
				cout <<endl;
			}
			else Try(i + 1);
			c[j] = d1[i - j + n] = d2[i + j - 1] = 0;
		}
	}
}

int main(){
	cin >> n;
	Try(1);
	cout << res;
	return 0;
}
